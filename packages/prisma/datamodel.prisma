type Character {
  id: ID! @unique
  marvelId: String! @unique
  createdAt: DateTime!
  updatedAt: DateTime!
  resourceURI: String
  thumbnail: String
  modified: String
  name: String
  description: String
  urls: Json
  comics: [Comic!]
  series: [Series!]!
  events: [Event!]!
  stories: [Story!]!
}

type Comic {
  id: ID! @unique
  marvelId: String! @unique
  createdAt: DateTime!
  updatedAt: DateTime!
  resourceURI: String
  thumbnail: String
  modified: String
  digitalId: Int
  title: String
  issueNumber: Int
  variantDescription: String
  description: String
  isbn: String
  upc: String
  diamondCode: String
  ean: String
  issn: String
  format: String
  textObjects: Json
  urls: Json
  dates: Json
  prices: Json
  images: Json
  characters: [Character!]!
  events: [Event!]!
  stories: [Story!]! @relation(name: "ComicsStories")
  creators: [Creator!]!
  series: Series
  # variants: [Story!] @relation(name: "StoryOriginalIssue")
  # _variants: Json
  # collections: [Summary!]
  # collectedIssues: [Summary!]
}

type Creator {
  id: ID! @unique
  marvelId: String! @unique
  createdAt: DateTime!
  updatedAt: DateTime!
  resourceURI: String
  thumbnail: String
  modified: String
  firstName: String
  middleName: String
  lastName: String
  suffix: String
  fullName: String
  urls: Json
  series: [Series!]!
  stories: [Story!]!
  comics: [Comic!]!
  events: [Event!]!
}

type Event {
  id: ID! @unique
  marvelId: String! @unique
  createdAt: DateTime!
  updatedAt: DateTime!
  resourceURI: String
  thumbnail: String
  modified: String
  title: String
  description: String
  urls: Json
  start: DateTime
  end: DateTime
  series: [Series!]!
  stories: [Story!]!
  comics: [Comic!]!
  characters: [Character!]!
  creators: [Creator!]!
  next: Event @relation(name: "EventRelation")
  previous: Event @relation(name: "EventRelation")
}

type Series {
  id: ID! @unique
  marvelId: String! @unique
  createdAt: DateTime!
  updatedAt: DateTime!
  resourceURI: String
  thumbnail: String
  modified: String
  title: String
  description: String
  urls: Json
  startYear: Int
  endYear: Int
  rating: String
  type: String
  comics: [Comic!]!
  stories: [Story!]!
  events: [Event!]!
  characters: [Character!]!
  creators: [Creator!]!
  next: Series @relation(name: "SeriesRelation")
  previous: Series @relation(name: "SeriesRelation")
}

type Story {
  id: ID! @unique
  marvelId: String! @unique
  createdAt: DateTime!
  updatedAt: DateTime!
  resourceURI: String
  thumbnail: String
  modified: String
  title: String
  description: String
  type: String
  comics: [Comic!]! @relation(name: "ComicsStories")
  events: [Event!]!
  characters: [Character!]!
  creators: [Creator!]!
  series: [Series!]!
  # originalIssue: Comic @relation(name: "StoryOriginalIssue")
  _originalIssue: Json
}
