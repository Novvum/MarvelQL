{
  "compilerOptions": {
    "esModuleInterop": true,
    "target": "es5",
    "moduleResolution": "node",
    "module": "commonjs",
    "sourceMap": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": false,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "**/generated/**/*.*"
  ]
}