{
  "version": 2,
  "name": "marvel-graphql",
  "alias": "api.marvelql.com",
  "env": {
    "PRISMA_ENDPOINT": "prisma-endpoint"
  },
  "builds": [
    {
      "src": "packages/graphql/index.ts",
      "use": "@now/node-server@canary"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/packages/graphql"
    }
  ]
}